{{!
    @template local_mxschool/index

    The purpose of this template is to render an index of links in an unordered list.
}}

<div class="mx-index">
    <ul>
    {{# links }}
        <li><a href="{{ url }}">{{ text }}</a></li>
    {{/ links }}
    </ul>
</div>
